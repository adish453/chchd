<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
    	<managed-bean>
    		<managed-bean-name>
    		browseRequestBean</managed-bean-name>
    		<managed-bean-class>
    		edu.chc.helpdesk.admin.BrowseRequestBean</managed-bean-class>
		<managed-bean-scope>
		request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>
		browseResults</display-name>
		<from-view-id>
		/browseResults.jsp</from-view-id>
		<navigation-case>
		<from-outcome> success </from-outcome>
			<to-view-id>
			/browseRequest.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>
		browseRequest</display-name>
		<from-view-id>
		/browseRequest.jsp</from-view-id>
		<navigation-case>
		<from-outcome> success </from-outcome>
			<to-view-id>
			/browseResults.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>
		browseRequest</display-name>
		<from-view-id>
		/browseRequest.jsp</from-view-id>
		<navigation-case>
		<from-outcome> reset </from-outcome> 
			<to-view-id>
			/browseRequest.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
